<h1>Login form</h1>
<%= form_tag staff_session do %>
<div>
  <%= text_field_tag :email, nil, placeholder: 'example@gmail.com', required: '', type: "email"%>
</div>
<div>
  <%= password_field_tag :password, nil, placeholder: 'Password' %>
</div>
<%= submit_tag "Sign In", class: 'btn', id: "login" %>
<% end %>
